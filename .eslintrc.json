{
  "extends": [
    "airbnb",
    "prettier"
  ],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "prettier/prettier": [
      "error"
    ],
    "react/jsx-filename-extension": 0,
    "import/extensions": 0,
    "no-param-reassign": 0,
    "react/jsx-props-no-spreading": 0,
    "import/prefer-default-export": 0,
    "react/jsx-one-expression-per-line": 0,
    "import/no-extraneous-dependencies": [
      "error"
    ]
  }
}
